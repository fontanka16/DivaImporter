@{
    string controllerName = HttpContext.Current.Request.RequestContext.RouteData.Values["controller"].ToString();
}
<!DOCTYPE html>
<html lang="sv-se">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <title>@ViewBag.Title </title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/Own.css" rel="stylesheet" />
</head>
<body role="document">
    <!-- Fixed navbar -->
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>               
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="@Url.Action("Index", "Home")"><span class="glyphicon glyphicon-home" aria-hidden="true"></span> Hem </a></li>
                    <li><a href="@Url.Action("EditSettings", "WoSImporting")"><span class="glyphicon glyphicon-cog" aria-hidden="true"></span> Inställningar </a></li>                               
                </ul>
                <p class="navbar-text navbar-right">
                    <a aria-label="Star StockholmUniversityLibrary/DivaImporter on GitHub" data-count-aria-label="# stargazers on GitHub" data-count-api="/repos/StockholmUniversityLibrary/DivaImporter#stargazers_count" data-count-href="/StockholmUniversityLibrary/DivaImporter/stargazers" data-style="mega" href="https://github.com/StockholmUniversityLibrary/DivaImporter" class="github-button">Gilla</a>
                    <a aria-label="Issue StockholmUniversityLibrary/DivaImporter on GitHub" data-count-aria-label="# issues on GitHub" data-count-api="/repos/StockholmUniversityLibrary/DivaImporter#open_issues_count" data-style="mega" href="https://github.com/StockholmUniversityLibrary/DivaImporter/issues" class="github-button">Önskemål och buggar</a>
                </p>
            </div><!--/.nav-collapse -->
           
        </div>
    </nav>
    <div class="container theme-showcase" role="main">
        @RenderBody()
    </div>
    <script src="~/Scripts/jquery-2.1.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/Scripts.js"></script>
    <script async defer src="https://buttons.github.io/buttons.js"></script>
</body>
</html>
